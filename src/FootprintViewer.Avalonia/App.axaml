<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:FootprintViewer.Avalonia"
             x:Class="FootprintViewer.Avalonia.App">
  
  <Application.DataTemplates>
    <local:ViewLocator/>
  </Application.DataTemplates>

  <Application.Styles>
    
    <FluentTheme Mode="Dark"/>

    <StyleInclude Source="avares://FootprintViewer.Avalonia/Themes/ThemeStyle.axaml"/>
    <StyleInclude Source="avares://FootprintViewer.Avalonia/Icons.axaml"/>
    <StyleInclude Source="avares://FootprintViewer.Avalonia/Styles/FastToolTip.axaml"/>
  
  </Application.Styles>
  
</Application>
